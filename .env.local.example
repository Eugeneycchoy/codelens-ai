# Backend Configuration (for local development)
# Copy this file to .env.local and fill in your values

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your-azure-openai-api-key
AZURE_OPENAI_ENDPOINT=https://eugene-openai-dev.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT=gpt-4o-mini
AZURE_OPENAI_API_VERSION=2024-08-06

# Backend Security
JWT_SECRET=your-jwt-secret-32-chars-minimum
SHARED_API_KEY=your-shared-api-key-32-chars-minimum

# Azure Resources
# Redis: set either REDIS_CONNECTION_STRING or all of REDIS_HOST, REDIS_PORT, REDIS_PASSWORD
REDIS_CONNECTION_STRING=your-redis-connection-string
APPINSIGHTS_CONNECTION_STRING=your-appinsights-connection-string

REDIS_HOST=dumbify-redis.redis.cache.windows.net
REDIS_PORT=6380
REDIS_PASSWORD=YOUR_NEW_KEY_HERE

# Backend URL (after deployment)
BACKEND_URL=https://your-function-app.azurewebsites.net
